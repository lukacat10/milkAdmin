									<label><input type="radio" name="value" value="true"> lang1On</label>
									<label><input type="radio" name="value" value="false"> lang1Off</label> &nbsp; &nbsp;<input type="submit" value="lang1Change">
								</div>
							</form>
						</div>
						<!-- Finish Server Info Div -->
						
						<!-- Start Console Div -->
						<div id="consolediv" class="tab">
							<textarea name="comments" style="width: 99%; height:450px; background-color: black; color: white;" id="console" readonly="true"></textarea>
							
							<form name="input" id="consolesubmit" action="/server/say" method="get" onsubmit="return adminUser(this)">
								<input type="text" id="consolesubmitm" name="message" maxlength="50" style="width:88%">
								<input type="submit" style="width:10%" value="lang1Send">
							</form>
						</div>
						<!-- Finish Console Div -->
						
						<!-- Start Player Div -->
						<div id="playerdiv" class="tab">
							<div id="dialog-user" title="langUserManagement">
								<div>
									<span>langIP: </span><span class="ip" style="color:green"></span><span> | langPort: </span><span class="port" style="color:green"></span>
								</div>
								<hr/>
								<form name="user" action="/player/set_health" method="get" style="display:inline;" onsubmit="return adminUser(this)">
									<input type="hidden" name="user" value="">
									<input type="hidden" name="amount" value="0">
									<input type="submit" value="langKill">
								</form>
								<form name="user" action="/player/kick_user" method="get" style="display:inline;" onsubmit="return ban(this,'kick')">
									<input type="hidden" name="user" value="">
									<input type="submit" value="langKick">
								</form>
								<form name="user" action="/player/ban_player" method="get" style="display:inline;" onsubmit="return ban(this,'name')">
									<input type="hidden" name="user" value="">
									<input type="submit" value="langBanName">
								</form>
								<form name="user" action="/player/ban_ip" method="get" style="display:inline;" onsubmit="return ban(this,'ip')">
									<input type="hidden" name="ip" value="">
									<input type="submit" value="langBanIP">
								</form>
								<hr/>
								<span>langAmount: <input type="text" value="1" size="3" maxlength="3" onkeyup="$('form[name=\'shoot\'] input[name=\'amount\']').val(this.value)" onkeypress="return onlyNumbers(event);"></span>
								<form name="shoot" action="/player/shoot_arrow" method="get" style="display:inline;" onsubmit="return adminUser(this)">
									<input type="hidden" name="user" value="">
									<input type="hidden" name="amount" value="1">
									<input type="submit" value="langShootArrow">
								</form>
								<form name="shoot" action="/player/shoot_fireball" method="get" style="display:inline;" onsubmit="return adminUser(this)">
									<input type="hidden" name="user" value="">
									<input type="hidden" name="amount" value="1">
									<input type="submit" value="langShootFireball">
								</form>
								<br/><br/>
								<form name="shoot" action="/player/throw_egg" method="get" style="display:inline;" onsubmit="return adminUser(this)">
									<input type="hidden" name="user" value="">
									<input type="hidden" name="amount" value="1">
									<input type="submit" value="langThrowEgg">
								</form>
								<form name="shoot" action="/player/throw_snowball" method="get" style="display:inline;" onsubmit="return adminUser(this)">
									<input type="hidden" name="user" value="">
									<input type="hidden" name="amount" value="1">
									<input type="submit" value="langThrowSnowball">
								</form>
								<hr/>
								<form name="user" action="/player/teleport_to_player" method="get" onsubmit="return adminUser(this)">
									<input type="hidden" name="user" value="">
									<input type="submit" value="langTeleportToPlayer">
									<select id="usersoption" name="to_user" style="width:280px"></select>
								</form>
								<br>
								<form name="user" action="/player/teleport_to_location" method="get" onsubmit="return adminUser(this)">
									<input type="hidden" name="user" value="">
									<input type="submit" value="langTeleportToCoord">
									X: <input type="text" name="x" value="" size="4" maxlength="5"> Y: <input type="text" name="y" value="" size="4" maxlength="5"> Z: <input type="text" name="z" value="" size="4" maxlength="5">
								</form>
								<hr/>
								<form name="user" action="/player/change_display_name" method="get" onsubmit="return adminUser(this)">
									<input type="hidden" name="user" value="">
									<input type="submit" value="langChangeName">
									<input type="text" name="name" value="" size="43" maxlength="25">
								</form>
							</div>
							<div class="usersonline">
							</div>
						</div>
						<!-- Finish Player Div -->
						
						<!-- Start Banlist Div -->
						<div id="banlistdiv" class="tab">
							<div style="display:inline-block;padding-right:20px"><input type="button" name="banlistreload" onclick="reloadBanlist()" value="lang1ReloadList"></div>
							<div style="display:inline-block;padding-right:20px"><input type="text" id="banlistfilter" size="20" maxlength="50" onkeyup="banlistfilter(this.value)"> <input type="button" onclick="$('#banlistfilter').val('');banlistfilter('')" value="langClearFilter"></div>
							<input id="banlistadd" type="button" value="langCreateBan">
							<br><br>
							<div class="header"><span class="banlistuser">lang1Player</span><span class="banlistcause">langCause</span><span class="banlistbuttons">lang1Action</span></div>
							<div id="banlistsectionname" style="margin-bottom:10px">
								lang1ReloadList.
							</div>
							<div class="header"><span class="banlistuser">lang1IPAddress</span><span class="banlistcause">langCause</span><span class="banlistbuttons">lang1Action</span></div>
							<div id="banlistsectionip">
								lang1ReloadList.
							</div>
						</div>
						<!-- Finish Banlist Div -->
						
						<!-- Start Whitelist Div -->
						<div id="whitelistdiv" class="tab">
							<div style="margin-bottom:10px;display:inline-block;padding-right:10px"><input type="button" onclick="reloadWhiteList()" value="lang1ReloadList"></div>
							<div style="margin-top:10px;width:150px;display:inline-block;text-align:right;">langWLAddPlayer:</div>
							<div style="display:inline-block"><input type="text" id="WLAddPlayer" size="15" maxlength="50" class="actions" disabled="disabled" /> <input type="submit" value="lang1Add" onclick="addToWhiteList()" class="actions" disabled="disabled" /></div>
							<div style="margin-bottom:10px;display:inline-block;padding-right:20px"><input type="button" onclick="deleteWhiteList()" value="langWLDeleteSelected" class="actions" disabled="disabled" /></div>
							<div style="margin-bottom:10px;display:inline-block;"><input type="button" id="wlsave" onclick="saveWhiteList()" value="langWLSaveChanges" disabled="disabled" /></div>
							<select size="20" id="whitelistsel" name="users" multiple="multiple" style="width: 100%">
								<option value="">lang1ReloadList</option>
							</select>
						</div>
						<!-- Finish Whitelist Div -->
					</div>
					<!-- Finish Server Tab -->
				</div>
				<!-- Finish Server Div -->
				
				<!-- Start Plugin Div -->
				<div id="plugindiv" class="tab">
					<div style="width:200px;display:inline-block"><input type="button" name="pluginreload" onclick="reloadPluginList()" value="lang1ReloadList"></div>
					<form name="input" id="enableplugin" action="/server/enable_plugin" method="get" target="i" style="display:inline">
						<div style="margin-top:10px;width:150px;display:inline-block">langEnablePlugin:</div>
						<div style="display:inline-block"><input type="text" name="plugin" size="15" maxlength="50"> <input type="submit" value="langEnable"></div>
					</form>
					<div>
						<table id="pluginstable" style="width:910px">
							<thead>
								<tr>
									<th class="scolinfo" style="width: 20px;padding: 0 0 0 12px;"><div class="ui-icon ui-icon-power"></div></th>
									<th class="pluginname">langPluginName</th>
									<th class="pluginversion">langVersion</th>
									<th class="pluginbuttons">lang1Action</th>
								</tr>
							</thead>
							<tbody id="pluginscontents">
								<tr>
									<td>
										<span class="pluginstatus off"></span>
									</td>
									<td>langReloadTheList</td>
									<td>&nbsp;</td>
									<td>&nbsp;</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
				<!-- Finish Plugin Div -->
				
				<!-- Start Backup Div -->
				<div id="backupdiv" class="tab">
					<div style="margin-bottom:10px"><input type="button" name="backuplistreload" onclick="reloadBackupList()" value="lang1ReloadList"></div>
					<span>langChooseYourBackup:</span>
					<select size="7" id="backups" style="width: 100%"></select>
					<div id="backupz" style="margin-top: 10px;display:none;">
						<div id="restore" style="display: none">
							<a href="/restore=" style="text-decoration: none;">
								<div class="btn green">langRestoreBackup</div>
							</a>
						</div>
						<div id="delete" style="display: inline-block">
							<a href="/delete" target="i" style="text-decoration: none;">
								<div class="btn red">langDeleteBackup</div>
							</a>
						</div>
						<div>
							<input type="checkbox" id="backupClear" checked="true"/>
							langBackupClear
						</div>
					</div>
				</div>
				<!-- Finish Backup Div -->
				
				<!-- Start milkAdmin Div -->
				<div id="milkdiv" class="tab">
					<form name="input" id="register" action="/server/account_create" method="get" onsubmit="return adminUser(this)">
						langRegisterAdmin:<br />
						<div style="margin-top:10px;width:100px;display:inline-block">lang1User:</div>
						<div style="display:inline-block"><input type="text" name="username" size="15" maxlength="50"></div><br/>
						<div style="margin-top:10px;width:100px;display:inline-block">lang1Password:</div>
						<div style="display:inline-block"><input type="text" name="password" size="15" maxlength="50"></div><br/><br/>
						<input type="submit" value="langCreateAdmin">
					</form>
				</div>
				<!-- Finish milkAdmin Div -->
			</div>
		</div>
	</div>
</body>
</html>
